[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[svn-remote "svn"]
	url = https://xrzperl.googlecode.com/svn
	fetch = trunk:refs/remotes/trunk
	branches = branches/*:refs/remotes/*
	tags = tags/*:refs/remotes/tags/*
[svn]
	authorsfile = /myplace/workspace/gsbridge/authors.txt
[remote "gitorious"]
	url = git@gitorious.org:eotect/perl.git
	fetch = +refs/heads/*:refs/remotes/gitorious/*
[remote "github"]
	url = git@github.com:eotect/perl.git
	fetch = +refs/heads/*:refs/remotes/github/*
